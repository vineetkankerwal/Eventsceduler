				<script>
					document.addEventListener('DOMContentLoaded', function () {
document.querySelector('[data-bs-submit="modal"]').addEventListener('click', function () {
document.querySelector('#exampleModal form').submit();
});
});
	document.getElementById('convertButton').addEventListener('click', function () {
        const inputTime = document.getElementById('inputTime').value;
        const fromTimezone = document.getElementById('fromTimezone').value;
        const toTimezone = document.getElementById('toTimezone').value;

        if (!inputTime) {
            alert('Please enter a time.');
            return;
        }

        const date = new Date(inputTime);
        const utcTime = date.toISOString(); // Convert input to UTC
        const convertedTime = new Date(utcTime).toLocaleString('en-US', { timeZone: toTimezone });

        document.getElementById('result').innerText = `Converted Time: ${convertedTime}`;
    });

				</script>
